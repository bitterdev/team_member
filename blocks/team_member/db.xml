<?xml version="1.0" encoding="UTF-8"?>
<schema
        xmlns="http://www.concrete5.org/doctrine-xml/0.5"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">

    <table name="btTeamMember">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="fullName" type="string" size="255" />
        <field name="position" type="string" size="255" />
        <field name="email" type="string" size="255" />
        <field name="phone" type="string" size="255" />

        <field name="website" type="string" size="255" />
        <field name="xing" type="string" size="255" />
        <field name="linkedin" type="string" size="255" />
        <field name="facebook" type="string" size="255" />
        <field name="twitter" type="string" size="255" />
        <field name="instagram" type="string" size="255" />

        <field name="photoFileId" type="integer">
            <unsigned/>
        </field>
    </table>
</schema>